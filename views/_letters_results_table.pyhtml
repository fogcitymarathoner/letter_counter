
<?py if len(letters) > 0: ?>
<table>
    <th>Letter</th>
    <th>Url Encoded</th>
    <th>Count</th>

    <?py for l in letters: ?>
        <tr><td class="qstr">'${ l.letter }'</td><td class="code">#{ l.urlencode() }</td><td class="count">${ l.count }</td></tr>
    <?py #endfor ?>
<?py #endif ?>
</table>